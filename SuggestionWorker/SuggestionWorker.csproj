    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>dotnet-SuggestionWorker-a11d2731-feb4-40c0-a77f-74890c67a9ce</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
